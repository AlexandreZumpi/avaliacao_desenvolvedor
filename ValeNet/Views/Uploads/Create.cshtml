@model IEnumerable<ValeNet.ModelsImports.DadosModelImports>
@{
    ViewData["Title"] = "Create";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Blank page
        <small>it all starts here</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="~/"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="Index">Lista de Importados</a></li>
        <li class="active">Carregar Arquivo</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

    <!-- Default box -->
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">UpLoad de Arquivo</h3>
        </div>
        <div class="box-body">
            <div class="alert alert-info">
                Selecione um arquivo que deseja Importar!
            </div>

            <form method="post" enctype="multipart/form-data" asp-controller="Uploads" asp-action="Create">
                <div class="form-group">
                    <div class="form-group">
                        <label for="exampleInputFile">Selecione um arquivo</label>
                        <input type="file" id="arquivo" name="arquivo" accept="text/plain">
                    </div>
                </div>

                <div>
                    <button type="reset" asp-action="index" class="btn btn-default">Retornar</button>
                    <button type="submit" class="btn btn-primary">Carregar</button>
                </div>
            </form>
        </div>
    </div>
    <!-- /.box -->

</section>
<!-- /.content -->